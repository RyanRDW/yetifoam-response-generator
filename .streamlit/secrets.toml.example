# Streamlit secrets configuration example
# Copy this file to secrets.toml and update with secure values

[auth]
# Generate secure passwords and update these values
staff_password = "SECURE_STAFF_PASSWORD_2024"
admin_password = "SECURE_ADMIN_PASSWORD_2024"
manager_password = "SECURE_MANAGER_PASSWORD_2024"

[app]
environment = "production"
debug_mode = false
max_results_limit = 15
rate_limit_queries_per_minute = 20

[monitoring]
enable_analytics = true
log_level = "INFO"

[data]
dataset_version = "v5.0"
quality_threshold = 0.75